{
  "name": "ash-library-game",
  "description": "Ashbury Library game built using Svelte and Sapper",
  "version": "0.1.0",
  "private": "true",
  "scripts": {
    "dev": "sapper dev --no-hot",
    "build": "sapper build",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "tokei": "tokei --exclude static/books.json"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.17.1",
    "sirv": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.12.5",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "cypress": "^6.1.0",
    "file-loader": "^6.2.0",
    "firebase": "^8.0.0",
    "postcss": "^8.1.4",
    "prettier": "2.1.2",
    "prettier-plugin-svelte": "1.4.0",
    "sapper": "^0.28.0",
    "sass": "^1.28.0",
    "svelte": "^3.31.0",
    "svelte-check": "^1.0.46",
    "svelte-loader": "^2.13.6",
    "svelte-materialify": "^0.3.0",
    "svelte-preprocess": "^4.3.0",
    "svelte-reduxify": "^1.1.0",
    "ts-loader": "^8.0.12",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-modules": "^1.0.0"
  }
}

{
  "name": "ash-library-game",
  "description": "Ashbury Library game built using Svelte and Sapper",
  "version": "0.1.0",
  "private": "true",
  "scripts": {
    "dev": "sapper dev --no-hot",
    "build": "cross-env NODE_ENV=production sapper build",
    "start": "node __sapper__/build",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "format": "prettier --write src && rustywind --write src",
    "format:check": "prettier --check src",
    "tokei": "tokei --exclude static/books.json"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "express": "^4.17.1",
    "sirv": "^1.0.11"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/node": "^14.14.16",
    "@types/polka": "^0.5.1",
    "autoprefixer": "^10.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "cypress": "^6.4.0",
    "file-loader": "^6.2.0",
    "firebase": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "postcss": "^8.2.4",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-nesting": "^7.0.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.1",
    "rustywind": "^0.8.0",
    "sapper": "^0.29.0",
    "sass": "^1.28.0",
    "style-loader": "^2.0.0",
    "svelte": "^3.31.0",
    "svelte-check": "^1.1.23",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^4.3.0",
    "svelte-reduxify": "^1.3.0",
    "tailwindcss": "^2.0.2",
    "ts-loader": "^8.0.14",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-modules": "^1.0.0"
  }
}

{
  "name": "ash-library-game",
  "description": "Ashbury Library game built using Svelte and Sapper",
  "version": "0.1.0",
  "private": "true",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "validate": "svelte-check --ignore src/node_modules/@sapper",
    "format": "prettier --write src && rustywind --write src",
    "format:check": "prettier --check src",
    "tokei": "tokei --exclude static/books.json"
  },
  "type": "module",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@sveltejs/adapter-vercel": "^1.0.0-next.8",
    "@sveltejs/kit": "^1.0.0-next.61",
    "@tailwindcss/jit": "^0.1.1",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.25",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "cypress": "^6.4.0",
    "firebase": "^8.3.1",
    "postcss": "^8.2.7",
    "postcss-import": "^14.0.0",
    "postcss-load-config": "^3.0.1",
    "postcss-nesting": "^7.0.1",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.1.3",
    "rustywind": "^0.8.1",
    "rxfire": "^4.0.0",
    "rxjs": "^6.6.7",
    "sass": "^1.32.7",
    "svelte": "^3.35.0",
    "svelte-check": "^1.1.33",
    "svelte-loader-hot": "^0.3.1",
    "svelte-preprocess": "^4.6.9",
    "svelte-reduxify": "^1.3.0",
    "tailwindcss": "^2.0.4",
    "tslib": "^2.1.0",
    "typescript": "^4.1.4",
    "vite": "^2.1.3"
  }
}
